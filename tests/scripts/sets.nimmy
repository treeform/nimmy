# Test set operations

# Create sets
let a = {1, 2, 3}
let b = {2, 3, 4}
let c = {1, 2, 3}

# Display sets
echo a
echo b

# Cardinality (UFCS style)
echo a.card
echo b.card

# Empty set
let empty = {}
echo empty.card

# Contains / in operator (UFCS style)
echo 2 in a
echo 5 in a
echo a.contains(3)
echo a.contains(10)

# Set union (+)
let unionSet = a + b
echo unionSet.card
echo 1 in unionSet
echo 4 in unionSet

# Set intersection (*)
let intersectSet = a * b
echo intersectSet.card
echo 2 in intersectSet
echo 1 in intersectSet

# Set difference (-)
let diffSet = a - b
echo diffSet.card
echo 1 in diffSet
echo 2 in diffSet

# Modify set with incl/excl (UFCS style)
var s = {10, 20}
s.incl(30)
echo s.card
echo 30 in s

s.excl(20)
echo s.card
echo 20 in s

# Duplicate handling (sets should dedupe)
let dupes = {1, 1, 2, 2, 3}
echo dupes.card

# Set equality
echo a == c
echo a == b

# Mixed types in set
let mixed = {1, 2, 3}
echo 2 in mixed
