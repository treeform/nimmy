# Array creation
let numbers = [1, 2, 3, 4, 5]
let names = ["Alice", "Bob", "Charlie"]

echo numbers.len
echo names.len

# Array indexing
echo numbers[0]
echo numbers[2]
echo names[1]

# Array iteration
echo "Numbers:"
for n in numbers:
  echo n

# Array modification
var items = [10, 20, 30]
items[1] = 25
echo items[1]

# Add and pop (UFCS style)
var stack = [1, 2]
stack.add(3)
echo stack.len
let top = stack.pop()
echo top
echo stack.len
