# Test table operations

# Create a table
let person = {"name": "Alice", "age": "30"}
echo person["name"]
echo person["age"]

# Modify table
var scores = {"math": "90", "english": "85"}
scores["science"] = "95"
echo scores["science"]

# Check if key exists (UFCS style)
echo scores.hasKey("math")
echo scores.hasKey("history")

# Using in operator for key check
echo "math" in scores
echo "history" in scores

# Get keys and values (UFCS style)
let k = scores.keys
echo k.len

let v = scores.values
echo v.len

# Delete a key (UFCS style)
scores.del("english")
echo scores.hasKey("english")
echo scores.keys.len

# Empty table
let empty = {"temp": "x"}
empty.del("temp")
echo empty.keys.len

# Contains function (UFCS style)
echo scores.contains("math")
echo scores.contains("history")

# Print tables
let simple = {"a": 1, "b": 2}
echo simple

let nested = {"outer": {"inner": "value"}}
echo nested

let mixed = {"name": "test", "count": 42, "flag": true}
echo mixed

# Empty table display
let emptyTable = {"x": 1}
emptyTable.del("x")
echo emptyTable
